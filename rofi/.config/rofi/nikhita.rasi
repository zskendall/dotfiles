/**
 * Nikhita Colors to be used by ROFI themes
 */

@import "general.rasi"

* {
  primary: #a402a5;
  primary-alt: #5d078680;
  text: #999;
  accent: #000;
  background: @grey-dark-50;
  background-alt: @primary-alt;
  selected-background: #00ff0099;
  active-foreground: @primary;
  border-color:           @grey;
}
